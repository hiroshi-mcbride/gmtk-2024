config_version=5

[application]

config/name="Void Bunny"
run/main_scene="res://game_root.tscn"
config/features=PackedStringArray("4.3", "Forward Plus")
boot_splash/bg_color=Color(0, 0, 0, 1)
config/icon="res://icon.svg"

[audio]

buses/default_bus_layout="res://assets/audio/default_bus_layout.tres"
manager/sound/bus="Sound"
manager/sound/pool_1D_size=16
manager/sound/pool_2D_size=16
manager/sound/pool_3D_size=16
manager/sound/max_polyphony=32
manager/music/bus="Music"

[autoload]

GlobalSignals="*res://src/global/global_signals.gd"
Strings="*res://src/global/strings.gd"
InputHelper="*res://addons/input_helper/input_helper.gd"
SoundManager="*res://addons/resonate/sound_manager/sound_manager.gd"
MusicManager="*res://addons/resonate/music_manager/music_manager.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/mode=3
window/size/resizable=false
window/size/borderless=true
window/stretch/mode="viewport"
window/stretch/scale=2.0
window/stretch/scale_mode="integer"

[dotnet]

project/assembly_name="gmtk-2024"

[editor_plugins]

enabled=PackedStringArray("res://addons/input_helper/plugin.cfg", "res://addons/resonate/plugin.cfg")

